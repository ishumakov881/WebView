<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Theme.NeFake" parent="android:Theme.Material.Light.NoActionBar">
        <item name="android:statusBarColor">@android:color/transparent</item>
        <item name="android:navigationBarColor">@android:color/transparent</item>
    </style>

    <style name="Theme.NeFake.NoActionBar">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

<!--    <style name="Theme.WebView" parent="android:Theme.Material.Light.NoActionBar">-->
    <style name="Theme.WebView" parent="android:Theme.Material.NoActionBar">

        <item name="android:statusBarColor">#ea0093</item>

        <!-- Цвет навигационного бара (нижние кнопки) -->
        <item name="android:navigationBarColor">#ea0093</item>

    </style>
</resources> 